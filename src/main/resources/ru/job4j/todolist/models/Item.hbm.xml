<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping>
    <class name="ru.job4j.todolist.models.Item" table="items">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="create" type="timestamp" column="create_time"/>
        <property name="description"/>
        <property name="done"/>
    </class>
</hibernate-mapping>